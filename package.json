{
  "name": "@stencil/starter",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil App Starter",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --prerender",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@stencil/core": "next",
    "@stencil/router": "latest"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil-app-starter.git"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ionic-team/stencil"
  },
  "homepage": "https://github.com/ionic-team/stencil",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "name": "@openforge/juntoscope",
    "private": true,
    "version": "0.0.1",
    "description": "Juntoscope Web App",
    "files": [
      "dist/"
    ],
    "scripts": {
      "build": "stencil build --prerender",
      "cz": "git-cz",
      "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
      "lint": "tslint -p ./tsconfig.json --exclude '**/*.d.ts'",
      "serve": "stencil-dev-server",
      "start": "npm run dev",
      "test": "jest --no-cache",
      "test.watch": "jest --watch --no-cache",
      "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
    },
    "dependencies": {
      "@stencil/core": "^0.7.2",
      "@stencil/router": "latest"
    },
    "devDependencies": {
      "@commitlint/cli": "^6.1.3",
      "@commitlint/config-conventional": "^6.1.3",
      "@stencil/dev-server": "latest",
      "@stencil/utils": "latest",
      "@types/jest": "^21.1.1",
      "commitizen": "^2.9.6",
      "conventional-changelog-cli": "^1.3.16",
      "cz-conventional-changelog": "^2.1.0",
      "husky": "^0.15.0-rc.12",
      "jest": "^21.2.1",
      "lint-staged": "^7.0.0",
      "prettier": "^1.11.1",
      "tslint": "^5.9.1",
      "tslint-config-airbnb": "^5.8.0",
      "tslint-config-prettier": "^1.10.0",
      "tslint-react": "^3.5.1"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/openforge/juntoscope.git"
    },
    "author": "OpenForge Team",
    "license": "Apache 2.0",
    "bugs": {
      "url": "https://github.com/openforge/juntoscope"
    },
    "homepage": "https://www.openforge.io",
    "jest": {
      "transform": {
        "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
      },
      "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
      "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "json",
        "jsx"
      ]
    },
    "config": {
      "commitizen": {
        "path": "./node_modules/cz-conventional-changelog"
      }
    },
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged",
        "commit-msg": "commitlint -e $GIT_PARAMS"
      }
    },
    "lint-staged": {
      "src/**/*.{js,jsx,ts,tsx}": [
        "npm run lint",
        "git add"
      ],
      "src/**/*.{js,jsx,ts,tsx,css,scss,md}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "json",
    "jsx"
  ]
}
